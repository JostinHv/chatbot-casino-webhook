# Configuración del servidor
NODE_ENV=development
PORT=3000

# Configuración de la base de datos MySQL
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=tu_password
DB_NAME=chatbot_casino
DB_CONNECTION_LIMIT=10

# Configuración de logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Configuración de seguridad
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configuración de Dialogflow (opcional para pruebas)
DIALOGFLOW_PROJECT_ID=tu-proyecto-dialogflow
DIALOGFLOW_PRIVATE_KEY_ID=tu-private-key-id
DIALOGFLOW_PRIVATE_KEY=tu-private-key
DIALOGFLOW_CLIENT_EMAIL=tu-client-email
DIALOGFLOW_CLIENT_ID=tu-client-id 